{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "devices": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "emulator_address": {
                        "type": "string"
                    },
                    "adb_path": {
                        "type": "string"
                    },
                    "start_path": {
                        "type": "string"
                    }
                },
                "additionalProperties": false,
                "required": [
                    "emulator_address",
                    "adb_path"
                ]
            }
        },
        "plan": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "time": {
                        "type": "integer"
                    },
                    "task": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "enum": [
                                "skland",
                                "maa"
                            ]
                        }
                    }
                },
                "additionalProperties": false,
                "required": [
                    "time",
                    "task"
                ]
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "devices",
        "plan"
    ]
}