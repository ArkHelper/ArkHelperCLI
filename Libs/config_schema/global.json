{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "devices": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "emulator_address": {
                        "type": "string"
                    },
                    "adb_path": {
                        "type": "string"
                    },
                    "start_path": {
                        "type": "string"
                    }
                },
                "additionalProperties": false,
                "required": [
                    "emulator_address",
                    "adb_path"
                ]
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "devices"
    ]
}